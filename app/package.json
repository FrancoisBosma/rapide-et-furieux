{
  "name": "vitesse-poc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "run-p lint:*",
    "lint:types": "vue-tsc --noEmit",
    "lint:css": "stylelint --fix \"**/*.{css,scss,vue}\"",
    "lint:js": "eslint --fix --ext \"**/*.{js,ts,vue}\"",
    "lint:imports": "import-sort --write \"src/**/*{.js,.ts,.vue}\"",
    "lint:md": "markdownlint-cli2 --config .markdownlint.json --fix \"src/(features/|global/components)/**/!(README).md\"",
    "lint:prettier": "prettier --write --loglevel error \"**/*.{js,ts,json,css,scss,vue,html,md}\"",
    "preinstall": "npx only-allow pnpm",
    "prepare": "cd .. && husky install && chmod ug+x .husky/*",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "sec-audit": "run-p sec-audit:*",
    "sec-audit:dev": "pnpm audit --json --dev",
    "sec-audit:prod": "pnpm audit --json --prod",
    "sec-audit:force:fix": "pnpm audit --fix",
    "XXXtest:ci": "run-p test:ci:*",
    "XXXtest:ci:components": "cypress run-ct",
    "XXXtest:ci:e2e": "cypress run --headless",
    "XXXtest:local": "run-p test:local:*",
    "XXXtest:local:components": "cypress open-ct",
    "XXXtest:local:e2e": "cypress open"
  },
  "dependencies": {
    "@vueuse/core": "^6.9.2",
    "@vueuse/head": "^0.6.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.6",
    "prism-theme-vars": "^0.2.2",
    "vue": "^3.2.24",
    "vue-demi": "^0.11.4",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.10.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^2.2.3",
    "@iconify-json/carbon": "^1.0.12",
    "@intlify/vite-plugin-vue-i18n": "^2.5.0",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.24",
    "@vue/server-renderer": "^3.2.24",
    "critters": "^0.0.14",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "https-localhost": "^4.7.0",
    "husky": "^7.0.4",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-config": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-eslint": "^6.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.2.1",
    "markdownlint-cli2": "^0.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.3",
    "unplugin-auto-import": "^0.4.20",
    "unplugin-icons": "^0.12.23",
    "unplugin-vue-components": "^0.17.9",
    "vite": "^2.7.1",
    "vite-plugin-inspect": "^0.3.11",
    "vite-plugin-md": "^0.11.4",
    "vite-plugin-pages": "^0.18.2",
    "vite-plugin-pwa": "^0.11.11",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.5.4",
    "vite-ssg": "^0.15.6",
    "vue-tsc": "^0.28.10"
  }
}
